      "endpoints": [
        "https://github.com/JonasPalms/time-tracker/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhBMTJFQkY3MjU4RTE5ODMKUldTREdZNGw5K3NTaXZKUGJvRzZ1VG9YOG5mM2tma1IraExHSUt4Tll6Slh0YkRXb3FwWjllUlMK"
